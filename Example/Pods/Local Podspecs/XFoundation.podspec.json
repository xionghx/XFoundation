{
  "name": "XFoundation",
  "version": "0.1.5",
  "summary": "A short description of XFoundation.",
  "description": "Guide for private pods",
  "homepage": "https://github.com/xionghx/XFoundation",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "xionghx": "xionghuanxin@hotmail.com"
  },
  "platforms": {
    "ios": "9.0"
  },
  "swift_version": "5.0",
  "source": {
    "git": "https://github.com/xionghx/XFoundation.git",
    "tag": "0.1.5"
  },
  "source_files": "XFoundation/Classes/*.{h,m,swift}",
  "default_subspecs": [
    "Core",
    "Alert",
    "AttributedString"
  ],
  "subspecs": [
    {
      "name": "Core",
      "source_files": [
        "XFoundation/Classes/Core/*.{h,m,swift}",
        "XFoundation/Classes/*.{h,m,swift}"
      ],
      "dependencies": {
        "DeviceKit": [
          "~> 1.11.0"
        ]
      }
    },
    {
      "name": "Alert",
      "source_files": "XFoundation/Classes/Alert/*.{h,m,swift}",
      "dependencies": {
        "MBProgressHUD": [
          "~> 1.1.0"
        ]
      }
    },
    {
      "name": "AttributedString",
      "source_files": "XFoundation/Classes/AttributedString/*.{h,m,swift}",
      "dependencies": {
        "XFoundation/Core": [

        ]
      }
    }
  ]
}
